{
  "name": "promised-map",
  "version": "0.1.0",
  "description": "A map of promises that can be resolved or rejected by key",
  "author": {
    "name": "Vitaliy Potapov",
    "email": "noginsk@rambler.ru"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/vitalets/promised-map.git"
  },
  "engines": {
    "node": ">=8"
  },
  "main": "src/index.js",
  "browser": "dist/bundle.js",
  "scripts": {
    "lint": "eslint src test",
    "build": "rollup -c",
    "test": "mocha test/setup.js test/specs",
    "test:bundle": "LIB_PATH=../dist/bundle.js npm t",
    "ci": "run-s lint build test test:bundle",
    "toc": "markdown-toc README.md -i",
    "prerelease": "run-s lint build test test:bundle",
    "release": "npm version $VER && npm publish",
    "postrelease": "git push --follow-tags --no-verify",
    "release-patch": "VER=patch npm run release",
    "release-minor": "VER=minor npm run release"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged && npm test",
      "pre-push": "run-s lint test"
    }
  },
  "lint-staged": {
    "{src,test}/**/*.js": "eslint"
  },
  "devDependencies": {
    "@babel/core": "^7.6.4",
    "@babel/preset-env": "^7.6.3",
    "chai": "^4.2.0",
    "eslint": "^6.5.1",
    "eslint-plugin-mocha": "^6.2.0",
    "husky": "^3.0.9",
    "lint-staged": "^9.4.2",
    "markdown-toc": "^1.2.0",
    "mocha": "^6.2.2",
    "npm-run-all": "^4.1.5",
    "rollup": "^1.25.0",
    "rollup-plugin-babel": "^4.3.3",
    "rollup-plugin-banner": "^0.2.1",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-delete": "^1.1.0",
    "rollup-plugin-node-resolve": "^5.2.0"
  },
  "keywords": [
    "promise",
    "promise-api",
    "promises"
  ],
  "license": "MIT",
  "dependencies": {
    "promise-controller": "^0.6.0"
  }
}
